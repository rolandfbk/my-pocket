<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="app_header">
    <%= link_to "Back", splash_index_path %>
    <h2>Login</h2>
    <%= f.submit "Log in" %>
  </div>
  <div class="post_form_element">
      <%= f.email_field :email, class:"input_from", placeholder: "Email", autofocus: true %>
      <%= f.password_field :password, class:"input_from", placeholder: "Password" %>
  </div>
<% end %>
